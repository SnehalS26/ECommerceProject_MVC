<div id="carouselExampleFade" class="carousel slide carousel-fade" data-bs-ride="true">
    
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="https://c4.wallpaperflare.com/wallpaper/638/601/313/lipstick-shadows-black-background-brush-cosmetics-hd-wallpaper-preview.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="https://wallpapercave.com/wp/wp9492759.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="https://wallpapercave.com/wp/wp1866305.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="https://wallpapercave.com/wp/wp5262757.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="https://wallpapercave.com/wp/wp4041469.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="https://cdn.wallpapersafari.com/18/87/iPB0Im.png" class="d-block w-100" alt="...">
        </div>
